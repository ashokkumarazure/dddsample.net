<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="DomainDrivenDelivery.Domain" xmlns="urn:nhibernate-mapping-2.2" default-access="field">
  <class name="DomainDrivenDelivery.Domain.Model.Locations.Location" table="Location">
    <id name="_primaryKey" column="id">
      <generator class="NHibernate.Id.IdentityGenerator"/>
    </id>
    <component name="_unLocode" unique="true" update="false">
      <property name="_unLocode" column="unlocode" not-null="true"/>
    </component>
    <property name="_name" column="name" not-null="true"/>
  </class>
</hibernate-mapping>